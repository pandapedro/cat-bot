{
	"compilerOptions": {
		"lib": ["ES6"],
		"strict": true,
		"outDir": "dist",
		"target": "es2016",
		"module": "commonjs",
		"skipLibCheck": true,
		"removeComments": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"typeRoots": ["./src/@types", "./node_modules/@types"]
	},
	"exclude": ["dist", "node_modules", "package-lock.json", "package.json"]
}
